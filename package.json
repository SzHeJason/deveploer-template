{
  "name": "wesing-middleware",
  "version": "1.0.0",
  "description": "运营中台 wesing 项目转接层",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env PROTOBUF_PATH=$(pwd)/__PROTO__ nodemon",
    "debug": "cross-env INSPECT='--inspect' nodemon",
    "build": "tsc -p tsconfig.json",
    "check": "tsc --noEmit",
    "clean": "rm -rf node_modules && rm -rf dist",
    "lint-staged": "lint-staged -d",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "compile-schemas": "json2ts -i ./src/schemas -o ./src/types"
  },
  "keywords": [],
  "author": "kurthe",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.2.0",
    "@grpc/proto-loader": "^0.5.5",
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.13",
    "@types/pino": "^6.3.0",
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "fastify": "^3.1.1",
    "fastify-cors": "^4.1.0",
    "fastify-plugin": "^2.0.1",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.19",
    "pino": "^6.4.1",
    "type-fest": "^0.18.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "conventional-changelog-cli": "^2.0.34",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "json-schema-to-typescript": "^9.1.1",
    "lint-staged": "^10.2.9",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
